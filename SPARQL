PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX f: <http://www.semanticweb.org/cj/ontologies/2016/2/Ontology1456999826312.owl#>

SELECT ?film 
WHERE { 
	?film rdf:type f:FilmScreeningProgramme .
	?film f:hasScheduledDate "2014-08-15T22:00:00"^^xsd:dateTime .
}


	?film f:hasTitle ?title .
	?film f:hasScheduledDate ?date .

